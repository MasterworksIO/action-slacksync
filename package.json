{
  "name": "action-slacksync",
  "version": "1.2.0",
  "private": true,
  "description": "Sync job status with Slack",
  "main": "dist/index.js",
  "engines": {
    "node": "^16.13.0"
  },
  "prettier": "@masterworks/eslint-config-masterworks/prettier",
  "scripts": {
    "typecheck": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build src/main.ts",
    "test": "jest",
    "all": "npm run typecheck && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MasterworksIO/action-slacksync.git"
  },
  "keywords": [
    "actions",
    "node",
    "slack",
    "notification",
    "sync"
  ],
  "author": "Masterworks",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "0.6.1",
    "@actions/core": "1.9.1",
    "@actions/github": "5.0.0",
    "@octokit/types": "6.34.0",
    "@slack/web-api": "6.6.0",
    "loglevel": "1.8.0",
    "node-fetch": "3.2.0"
  },
  "devDependencies": {
    "@masterworks/eslint-config-masterworks": "github:MasterworksIO/eslint-config-masterworks#2.1.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.19",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "@vercel/ncc": "0.33.3",
    "eslint": "8.9.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
