{
  "name": "action-slacksync",
  "version": "1.5.0",
  "private": true,
  "description": "Sync job status with Slack",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": "^20.9.0",
    "pnpm": "^10.13.1"
  },
  "prettier": "@masterworks/eslint-config-masterworks/prettier/prettier.js",
  "scripts": {
    "typecheck": "tsc",
    "format": "prettier --write **/*.ts",
