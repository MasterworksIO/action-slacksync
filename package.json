  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@masterworks/eslint-config-masterworks": "github:MasterworksIO/eslint-config-masterworks#4.0.0",
    "@stylistic/eslint-plugin": "5.2.2",
    "@types/node": "24.2.0",
    "@vitest/eslint-plugin": "1.3.4",
    "eslint": "9.32.0",
